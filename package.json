{
  "name": "ts-ping",
  "version": "1.0.0",
  "description": "A ping wrapper for mode with TypeScript",
  "main": "./dist/source",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "ava",
    "build": "del-cli dist && tsc",
    "postbuild": "copyfiles -u 1 test/parser/samples/*.txt dist/test"
  },
  "author": "Govind S",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^13.13.5",
    "ava": "^3.8.2",
    "typescript": "^3.8.3",
    "@ava/typescript": "^1.1.1",
    "copyfiles": "^2.2.0"
  },
  "files": [
    "./dist/source",
    "./dist/test"
  ],
  "types": "./dist/src/types.d.ts",
  "ava": {
    "files": [
      "test/*",
      "test/builder/*",
      "test/parser/*"
    ],
    "timeout": "1m",
    "typescript": {
      "rewritePaths": {
        "test/": "dist/test/"
      }
    }
  },
  "dependencies": {
  }
}
